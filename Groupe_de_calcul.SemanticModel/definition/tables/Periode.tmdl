table Periode
	lineageTag: 5f193527-c046-4bae-a6d0-35f3bf720dab

	calculationGroup

		calculationItem Actuel = SELECTEDMEASURE()

		calculationItem YTD = TOTALYTD(SELECTEDMEASURE(), 'Donne_Vente'[Date])

		calculationItem MTD = TOTALMTD(SELECTEDMEASURE(), 'Donne_Vente'[Date])

		calculationItem '% Var. YTD N-1' =
				VAR Actuel = SELECTEDMEASURE()
				VAR AnDernier = CALCULATE(SELECTEDMEASURE(), SAMEPERIODLASTYEAR('Donne_Vente'[Date]))
				RETURN DIVIDE(Actuel - AnDernier, AnDernier)

